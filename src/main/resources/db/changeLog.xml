<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext
                   https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
                   http://www.liquibase.org/xml/ns/dbchangelog
                   https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd">

    <changeSet id="init_db" author="Ville Juutila">
        <createTable tableName="DailyEntry">
            <column name="entryId" type="VARCHAR(36)">
                <constraints primaryKey="true" />
            </column>
            <column name="forecastId" type="INT" />
            <column name="person" type="INT" />
            <column name="internalTime" type="INT" />
            <column name="projectTime" type="INT" />
            <column name="date" type="DATE" />
            <column name="createdAt" type="DATETIME" />
            <column name="updatedAt" type="DATETIME" />
        </createTable>
    </changeSet>
</databaseChangeLog>